                <outline text="from 表名1 left join 表名2 on 表名1.字段名 =  表名2.字段名" _mubu_text="%3Cspan%3Efrom%20%E8%A1%A8%E5%90%8D1%20left%20join%20%E8%A1%A8%E5%90%8D2%20on%20%E8%A1%A8%E5%90%8D1.%E5%AD%97%E6%AE%B5%E5%90%8D%20=%20%20%E8%A1%A8%E5%90%8D2.%E5%AD%97%E6%AE%B5%E5%90%8D%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="右连接" _mubu_text="%3Cspan%3E%E5%8F%B3%E8%BF%9E%E6%8E%A5%3C/span%3E" _note="" _mubu_note="">
                <outline text="select 字段名" _mubu_text="%3Cspan%3Eselect%20%E5%AD%97%E6%AE%B5%E5%90%8D%3C/span%3E" _note="" _mubu_note=""/>
