                <outline text="【运行代码】" _mubu_text="%3Cspan%3E%E3%80%90%E8%BF%90%E8%A1%8C%E4%BB%A3%E7%A0%81%E3%80%91%3C/span%3E" _note="" _mubu_note="">
                  <outline text="select" _mubu_text="%3Cspan%3Eselect%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="team1" _mubu_text="%3Cspan%3Eteam1%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text=",team2" _mubu_text="%3Cspan%3E,team2%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text=",player" _mubu_text="%3Cspan%3E,player%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="from game" _mubu_text="%3Cspan%3Efrom%20game%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="join goal" _mubu_text="%3Cspan%3Ejoin%20goal%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="on game.id = goal.matchid" _mubu_text="%3Cspan%3Eon%20game.id%20=%20goal.matchid%3C/span%3E" _note="" _mubu_note=""/>
                  <outline text="where player like &apos;Mario%&apos;" _mubu_text="%3Cspan%3Ewhere%20player%20like%20&apos;Mario%25&apos;%3C/span%3E" _note="" _mubu_note=""/>
