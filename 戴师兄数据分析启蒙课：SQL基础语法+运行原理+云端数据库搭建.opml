        <outline text="主知识点七：部分常见函数" _mubu_text="%3Cspan%3E%E4%B8%BB%E7%9F%A5%E8%AF%86%E7%82%B9%E4%B8%83%EF%BC%9A%E9%83%A8%E5%88%86%E5%B8%B8%E8%A7%81%E5%87%BD%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
          <outline text="【知识点引入】" _mubu_text="%3Cspan%3E%E3%80%90%E7%9F%A5%E8%AF%86%E7%82%B9%E5%BC%95%E5%85%A5%E3%80%91%3C/span%3E" _note="" _mubu_note="">
            <outline text="【数学函数】" _mubu_text="%3Cspan%3E%E3%80%90%E6%95%B0%E5%AD%A6%E5%87%BD%E6%95%B0%E3%80%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="round(x,y)——四舍五入函数" _mubu_text="%3Cspan%3Eround(x,y)%E2%80%94%E2%80%94%E5%9B%9B%E8%88%8D%E4%BA%94%E5%85%A5%E5%87%BD%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="round函数对x值进行四舍五入，精确到小数点后y位" _mubu_text="%3Cspan%3Eround%E5%87%BD%E6%95%B0%E5%AF%B9x%E5%80%BC%E8%BF%9B%E8%A1%8C%E5%9B%9B%E8%88%8D%E4%BA%94%E5%85%A5%EF%BC%8C%E7%B2%BE%E7%A1%AE%E5%88%B0%E5%B0%8F%E6%95%B0%E7%82%B9%E5%90%8Ey%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="y为负值时，保留小数点左边相应的位数为0，不进行四舍五入" _mubu_text="%3Cspan%3Ey%E4%B8%BA%E8%B4%9F%E5%80%BC%E6%97%B6%EF%BC%8C%E4%BF%9D%E7%95%99%E5%B0%8F%E6%95%B0%E7%82%B9%E5%B7%A6%E8%BE%B9%E7%9B%B8%E5%BA%94%E7%9A%84%E4%BD%8D%E6%95%B0%E4%B8%BA0%EF%BC%8C%E4%B8%8D%E8%BF%9B%E8%A1%8C%E5%9B%9B%E8%88%8D%E4%BA%94%E5%85%A5%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例如：round(3.15,1)返回3.2，round(14.15,-1)返回10" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Around(3.15,1)%E8%BF%94%E5%9B%9E3.2%EF%BC%8Cround(14.15,-1)%E8%BF%94%E5%9B%9E10%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="【字符串函数】" _mubu_text="%3Cspan%3E%E3%80%90%E5%AD%97%E7%AC%A6%E4%B8%B2%E5%87%BD%E6%95%B0%E3%80%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="concat(s1,s2,...)——连接字符串函数" _mubu_text="%3Cspan%3Econcat(s1,s2,...)%E2%80%94%E2%80%94%E8%BF%9E%E6%8E%A5%E5%AD%97%E7%AC%A6%E4%B8%B2%E5%87%BD%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="concat函数返回连接参数s1、s2等产生的字符串" _mubu_text="%3Cspan%3Econcat%E5%87%BD%E6%95%B0%E8%BF%94%E5%9B%9E%E8%BF%9E%E6%8E%A5%E5%8F%82%E6%95%B0s1%E3%80%81s2%E7%AD%89%E4%BA%A7%E7%94%9F%E7%9A%84%E5%AD%97%E7%AC%A6%E4%B8%B2%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="任一参数为null时，则返回null" _mubu_text="%3Cspan%3E%E4%BB%BB%E4%B8%80%E5%8F%82%E6%95%B0%E4%B8%BAnull%E6%97%B6%EF%BC%8C%E5%88%99%E8%BF%94%E5%9B%9Enull%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例如：concat(&apos;My&apos;,&apos; &apos;,&apos;SQL&apos;)返回My SQL，concat(&apos;My&apos;,null,&apos;SQL&apos;)返回null" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Aconcat(&apos;My&apos;,&apos;%20&apos;,&apos;SQL&apos;)%E8%BF%94%E5%9B%9EMy%20SQL%EF%BC%8Cconcat(&apos;My&apos;,null,&apos;SQL&apos;)%E8%BF%94%E5%9B%9Enull%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="replace(s,s1,s2)——替换函数" _mubu_text="%3Cspan%3Ereplace(s,s1,s2)%E2%80%94%E2%80%94%E6%9B%BF%E6%8D%A2%E5%87%BD%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="replace函数使用字符串s2代替s中所有的s1" _mubu_text="%3Cspan%3Ereplace%E5%87%BD%E6%95%B0%E4%BD%BF%E7%94%A8%E5%AD%97%E7%AC%A6%E4%B8%B2s2%E4%BB%A3%E6%9B%BFs%E4%B8%AD%3C/span%3E%3Cspan%20class=%22bold%22%3E%E6%89%80%E6%9C%89%3C/span%3E%3Cspan%3E%E7%9A%84s1%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例如：replace(&apos;MySQLMySQL&apos;,&apos;SQL&apos;,&apos;sql&apos;)返回MysqlMysql" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Areplace(&apos;MySQLMySQL&apos;,&apos;SQL&apos;,&apos;sql&apos;)%E8%BF%94%E5%9B%9EMysqlMysql%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="left(s,n)、right(s,n)&amp;substring(s,n,len)——截取字符串一部分的函数" _mubu_text="%3Cspan%3Eleft(s,n)%E3%80%81right(s,n)&amp;amp;substring(s,n,len)%E2%80%94%E2%80%94%E6%88%AA%E5%8F%96%E5%AD%97%E7%AC%A6%E4%B8%B2%E4%B8%80%E9%83%A8%E5%88%86%E7%9A%84%E5%87%BD%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="left函数返回字符串s最左边n个字符" _mubu_text="%3Cspan%3Eleft%E5%87%BD%E6%95%B0%E8%BF%94%E5%9B%9E%E5%AD%97%E7%AC%A6%E4%B8%B2s%E6%9C%80%E5%B7%A6%E8%BE%B9n%E4%B8%AA%E5%AD%97%E7%AC%A6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="right函数返回字符串s最右边n个字符" _mubu_text="%3Cspan%3Eright%E5%87%BD%E6%95%B0%E8%BF%94%E5%9B%9E%E5%AD%97%E7%AC%A6%E4%B8%B2s%E6%9C%80%E5%8F%B3%E8%BE%B9n%E4%B8%AA%E5%AD%97%E7%AC%A6%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="substring函数返回字符串s从第n个字符起取长度为len的子字符串，n也可以为负值，则从倒数第n个字符起取长度为len的子字符串，没有len值则取从第n个字符起到最后一位" _mubu_text="%3Cspan%3Esubstring%E5%87%BD%E6%95%B0%E8%BF%94%E5%9B%9E%E5%AD%97%E7%AC%A6%E4%B8%B2s%E4%BB%8E%E7%AC%ACn%E4%B8%AA%E5%AD%97%E7%AC%A6%E8%B5%B7%E5%8F%96%E9%95%BF%E5%BA%A6%E4%B8%BAlen%E7%9A%84%E5%AD%90%E5%AD%97%E7%AC%A6%E4%B8%B2%EF%BC%8Cn%E4%B9%9F%E5%8F%AF%E4%BB%A5%E4%B8%BA%3C/span%3E%3Cspan%20class=%22bold%22%3E%E8%B4%9F%E5%80%BC%3C/span%3E%3Cspan%3E%EF%BC%8C%E5%88%99%E4%BB%8E%3C/span%3E%3Cspan%20class=%22bold%22%3E%E5%80%92%E6%95%B0%3C/span%3E%3Cspan%3E%E7%AC%ACn%E4%B8%AA%E5%AD%97%E7%AC%A6%E8%B5%B7%E5%8F%96%E9%95%BF%E5%BA%A6%E4%B8%BAlen%E7%9A%84%E5%AD%90%E5%AD%97%E7%AC%A6%E4%B8%B2%EF%BC%8C%E6%B2%A1%E6%9C%89len%E5%80%BC%E5%88%99%E5%8F%96%E4%BB%8E%E7%AC%ACn%E4%B8%AA%E5%AD%97%E7%AC%A6%E8%B5%B7%E5%88%B0%E6%9C%80%E5%90%8E%E4%B8%80%E4%BD%8D%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例如：left(&apos;abcdefg&apos;,3)返回abc，right(&apos;abcdefg&apos;,3)返回efg，substring(&apos;abcdefg&apos;,2,3)返回bcd，substring(&apos;abcdefg&apos;,-2,3)返回fg，substring(&apos;abcdefg&apos;,2)返回bcdefg" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Aleft(&apos;abcdefg&apos;,3)%E8%BF%94%E5%9B%9Eabc%EF%BC%8Cright(&apos;abcdefg&apos;,3)%E8%BF%94%E5%9B%9Eefg%EF%BC%8Csubstring(&apos;abcdefg&apos;,2,3)%E8%BF%94%E5%9B%9Ebcd%EF%BC%8Csubstring(&apos;abcdefg&apos;,-2,3)%E8%BF%94%E5%9B%9Efg%EF%BC%8Csubstring(&apos;abcdefg&apos;,2)%E8%BF%94%E5%9B%9Ebcdefg%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="【数据类型转换函数】" _mubu_text="%3Cspan%3E%E3%80%90%E6%95%B0%E6%8D%AE%E7%B1%BB%E5%9E%8B%E8%BD%AC%E6%8D%A2%E5%87%BD%E6%95%B0%E3%80%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="cast(x as type)——转换数据类型的函数" _mubu_text="%3Cspan%3Ecast(x%20as%20type)%E2%80%94%E2%80%94%E8%BD%AC%E6%8D%A2%E6%95%B0%E6%8D%AE%E7%B1%BB%E5%9E%8B%E7%9A%84%E5%87%BD%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="cast函数将一个类型的x值转换为另一个类型的值" _mubu_text="%3Cspan%3Ecast%E5%87%BD%E6%95%B0%E5%B0%86%E4%B8%80%E4%B8%AA%E7%B1%BB%E5%9E%8B%E7%9A%84x%E5%80%BC%E8%BD%AC%E6%8D%A2%E4%B8%BA%E5%8F%A6%E4%B8%80%E4%B8%AA%E7%B1%BB%E5%9E%8B%E7%9A%84%E5%80%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="type参数可以填写char(n)、date、time、datetime、decimal等转换为对应的数据类型" _mubu_text="%3Cspan%3Etype%E5%8F%82%E6%95%B0%E5%8F%AF%E4%BB%A5%E5%A1%AB%E5%86%99char(n)%E3%80%81date%E3%80%81time%E3%80%81datetime%E3%80%81decimal%E7%AD%89%E8%BD%AC%E6%8D%A2%E4%B8%BA%E5%AF%B9%E5%BA%94%E7%9A%84%E6%95%B0%E6%8D%AE%E7%B1%BB%E5%9E%8B%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
            </outline>
            <outline text="【日期时间函数】" _mubu_text="%3Cspan%3E%E3%80%90%E6%97%A5%E6%9C%9F%E6%97%B6%E9%97%B4%E5%87%BD%E6%95%B0%E3%80%91%3C/span%3E" _note="" _mubu_note="">
              <outline text="year(date)、month(date)&amp;day(date)——获取年月日的函数" _mubu_text="%3Cspan%3Eyear(date)%E3%80%81month(date)&amp;amp;day(date)%E2%80%94%E2%80%94%E8%8E%B7%E5%8F%96%E5%B9%B4%E6%9C%88%E6%97%A5%E7%9A%84%E5%87%BD%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="date可以是年月日组成的日期，也可以是年月日时分秒组成的日期时间" _mubu_text="%3Cspan%3Edate%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%B9%B4%E6%9C%88%E6%97%A5%E7%BB%84%E6%88%90%E7%9A%84%E6%97%A5%E6%9C%9F%EF%BC%8C%E4%B9%9F%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%B9%B4%E6%9C%88%E6%97%A5%E6%97%B6%E5%88%86%E7%A7%92%E7%BB%84%E6%88%90%E7%9A%84%E6%97%A5%E6%9C%9F%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="year(date)返回日期格式中的年份" _mubu_text="%3Cspan%3Eyear(date)%E8%BF%94%E5%9B%9E%E6%97%A5%E6%9C%9F%E6%A0%BC%E5%BC%8F%E4%B8%AD%E7%9A%84%E5%B9%B4%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="month(date)返回日期格式中的月份" _mubu_text="%3Cspan%3Emonth(date)%E8%BF%94%E5%9B%9E%E6%97%A5%E6%9C%9F%E6%A0%BC%E5%BC%8F%E4%B8%AD%E7%9A%84%E6%9C%88%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="day(date)返回年日期格式中的日份" _mubu_text="%3Cspan%3Eday(date)%E8%BF%94%E5%9B%9E%E5%B9%B4%E6%97%A5%E6%9C%9F%E6%A0%BC%E5%BC%8F%E4%B8%AD%E7%9A%84%E6%97%A5%E4%BB%BD%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例如：year(&apos;2021-08-03&apos;)返回2021，month(&apos;2021-08-03&apos;)返回8，day(&apos;2021-08-03&apos;)返回3" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Ayear(&apos;2021-08-03&apos;)%E8%BF%94%E5%9B%9E2021%EF%BC%8Cmonth(&apos;2021-08-03&apos;)%E8%BF%94%E5%9B%9E8%EF%BC%8Cday(&apos;2021-08-03&apos;)%E8%BF%94%E5%9B%9E3%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="date_add(date,interval expr type)&amp;date_sub(date,interval expr type)——对指定起始时间进行加减操作" _mubu_text="%3Cspan%3Edate_add(date,interval%20expr%20type)&amp;amp;date_sub(date,interval%20expr%20type)%E2%80%94%E2%80%94%E5%AF%B9%E6%8C%87%E5%AE%9A%E8%B5%B7%E5%A7%8B%E6%97%B6%E9%97%B4%E8%BF%9B%E8%A1%8C%E5%8A%A0%E5%87%8F%E6%93%8D%E4%BD%9C%3C/span%3E" _note="" _mubu_note="">
                <outline text="date用来指定起始时间" _mubu_text="%3Cspan%3Edate%E7%94%A8%E6%9D%A5%E6%8C%87%E5%AE%9A%E8%B5%B7%E5%A7%8B%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="date可以是年月日组成的日期，也可以是年月日时分秒组成的日期时间" _mubu_text="%3Cspan%3Edate%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%B9%B4%E6%9C%88%E6%97%A5%E7%BB%84%E6%88%90%E7%9A%84%E6%97%A5%E6%9C%9F%EF%BC%8C%E4%B9%9F%E5%8F%AF%E4%BB%A5%E6%98%AF%E5%B9%B4%E6%9C%88%E6%97%A5%E6%97%B6%E5%88%86%E7%A7%92%E7%BB%84%E6%88%90%E7%9A%84%E6%97%A5%E6%9C%9F%E6%97%B6%E9%97%B4%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="expr用来指定从起始时间添加或减去的时间间隔" _mubu_text="%3Cspan%3Eexpr%E7%94%A8%E6%9D%A5%E6%8C%87%E5%AE%9A%E4%BB%8E%E8%B5%B7%E5%A7%8B%E6%97%B6%E9%97%B4%E6%B7%BB%E5%8A%A0%E6%88%96%E5%87%8F%E5%8E%BB%E7%9A%84%E6%97%B6%E9%97%B4%E9%97%B4%E9%9A%94%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="type指示expr被解释的方式，type可以可以是以下值" _mubu_text="%3Cspan%3Etype%E6%8C%87%E7%A4%BAexpr%E8%A2%AB%E8%A7%A3%E9%87%8A%E7%9A%84%E6%96%B9%E5%BC%8F%EF%BC%8Ctype%E5%8F%AF%E4%BB%A5%E5%8F%AF%E4%BB%A5%E6%98%AF%E4%BB%A5%E4%B8%8B%E5%80%BC%3C/span%3E" _note="" _mubu_note="">
                  <outline text="主要使用红框中的值" _mubu_text="%3Cspan%3E%E4%B8%BB%E8%A6%81%E4%BD%BF%E7%94%A8%E7%BA%A2%E6%A1%86%E4%B8%AD%E7%9A%84%E5%80%BC%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22oZopdBc2uC%22%2C%22oh%22%3A843%2C%22ow%22%3A361%2C%22uri%22%3A%22document_image%2F342ef187-60e2-4b9e-a0bd-f763812759a3-9404487.jpg%22%2C%22w%22%3A171%7D%5D"/>
                </outline>
                <outline text="date_add函数对起始时间进行加操作，date_sub函数对起始时间进行减操作" _mubu_text="%3Cspan%3Edate_add%E5%87%BD%E6%95%B0%E5%AF%B9%E8%B5%B7%E5%A7%8B%E6%97%B6%E9%97%B4%E8%BF%9B%E8%A1%8C%E5%8A%A0%E6%93%8D%E4%BD%9C%EF%BC%8Cdate_sub%E5%87%BD%E6%95%B0%E5%AF%B9%E8%B5%B7%E5%A7%8B%E6%97%B6%E9%97%B4%E8%BF%9B%E8%A1%8C%E5%87%8F%E6%93%8D%E4%BD%9C%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例如：date_add(&apos;2021-08-03 23:59:59&apos;,interval 1 second)返回2021-08-04 24:00:00，date_sub(&apos;2021-08-03 23:59:59&apos;,interval 2 month)返回2021-06-03 23:59:59" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Adate_add(&apos;2021-08-03%2023:59:59&apos;,interval%201%20second)%E8%BF%94%E5%9B%9E2021-08-04%2024:00:00%EF%BC%8Cdate_sub(&apos;2021-08-03%2023:59:59&apos;,interval%202%20month)%E8%BF%94%E5%9B%9E2021-06-03%2023:59:59%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="datediff(date1,date2)——计算两个日期之间间隔的天数" _mubu_text="%3Cspan%3Edatediff(date1,date2)%E2%80%94%E2%80%94%E8%AE%A1%E7%AE%97%E4%B8%A4%E4%B8%AA%E6%97%A5%E6%9C%9F%E4%B9%8B%E9%97%B4%E9%97%B4%E9%9A%94%E7%9A%84%E5%A4%A9%E6%95%B0%3C/span%3E" _note="" _mubu_note="">
                <outline text="datediff函数由date1-date2计算出间隔的时间，只有date的日期部分参与计算，时间不参与" _mubu_text="%3Cspan%3Edatediff%E5%87%BD%E6%95%B0%E7%94%B1date1-date2%E8%AE%A1%E7%AE%97%E5%87%BA%E9%97%B4%E9%9A%94%E7%9A%84%E6%97%B6%E9%97%B4%EF%BC%8C%E5%8F%AA%E6%9C%89date%E7%9A%84%E6%97%A5%E6%9C%9F%E9%83%A8%E5%88%86%E5%8F%82%E4%B8%8E%E8%AE%A1%E7%AE%97%EF%BC%8C%E6%97%B6%E9%97%B4%E4%B8%8D%E5%8F%82%E4%B8%8E%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="例如：datediff(&apos;2021-06-08&apos;,&apos;2021-06-01&apos;)返回7，datediff(&apos;2021-06-08 23:59:59&apos;,&apos;2021-06-01 21:00:00&apos;)返回7，datediff(&apos;2021-06-01&apos;,&apos;2021-06-08&apos;)返回-7" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Adatediff(&apos;2021-06-08&apos;,&apos;2021-06-01&apos;)%E8%BF%94%E5%9B%9E7%EF%BC%8Cdatediff(&apos;2021-06-08%2023:59:59&apos;,&apos;2021-06-01%2021:00:00&apos;)%E8%BF%94%E5%9B%9E7%EF%BC%8Cdatediff(&apos;2021-06-01&apos;,&apos;2021-06-08&apos;)%E8%BF%94%E5%9B%9E-7%3C/span%3E" _note="" _mubu_note=""/>
              </outline>
              <outline text="date_format(date,format)——将日期和时间格式化" _mubu_text="%3Cspan%3Edate_format(date,format)%E2%80%94%E2%80%94%E5%B0%86%E6%97%A5%E6%9C%9F%E5%92%8C%E6%97%B6%E9%97%B4%E6%A0%BC%E5%BC%8F%E5%8C%96%3C/span%3E" _note="" _mubu_note="">
                <outline text="date_format函数根据format指定的格式显示date值" _mubu_text="%3Cspan%3Edate_format%E5%87%BD%E6%95%B0%E6%A0%B9%E6%8D%AEformat%E6%8C%87%E5%AE%9A%E7%9A%84%E6%A0%BC%E5%BC%8F%E6%98%BE%E7%A4%BAdate%E5%80%BC%3C/span%3E" _note="" _mubu_note=""/>
                <outline text="可以换使用的格式有" _mubu_text="%3Cspan%3E%E5%8F%AF%E4%BB%A5%E6%8D%A2%E4%BD%BF%E7%94%A8%E7%9A%84%E6%A0%BC%E5%BC%8F%E6%9C%89%3C/span%3E" _note="" _mubu_note="">
                  <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22VrTt3FOoHg%22%2C%22oh%22%3A1160%2C%22ow%22%3A414%2C%22uri%22%3A%22document_image%2F3a48a766-8d12-49e5-9f53-8bec62b4791d-9404487.jpg%22%2C%22w%22%3A526%7D%5D"/>
                </outline>
                <outline text="例如：date_format(&apos;2018-06-01 16:23:12&apos;,&apos;%b %d %Y %h:%i %p&apos;)返回Jun 01 2018 04:23 PM，date_format(&apos;2018-06-01 16:23:12&apos;,&apos;%Y/%d/%m&apos;)返回2018/01/06" _mubu_text="%3Cspan%3E%E4%BE%8B%E5%A6%82%EF%BC%9Adate_format(&apos;2018-06-01%2016:23:12&apos;,&apos;%25b%20%25d%20%25Y%20%25h:%25i%20%25p&apos;)%E8%BF%94%E5%9B%9EJun%2001%202018%2004:23%20PM%EF%BC%8Cdate_format(&apos;2018-06-01%2016:23:12&apos;,&apos;%25Y/%25d/%25m&apos;)%E8%BF%94%E5%9B%9E2018/01/06%3C/span%3E" _note="" _mubu_note=""/>
